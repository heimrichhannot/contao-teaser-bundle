<?php if ($this->showMore): ?>
	<?php if($this->linkAll): ?>
		<span class="<?=$this->linkClass; ?>"><?= $this->link; ?></span>
	<?php else: ?>
		<a href="<?= $this->href; ?>" aria-lable="<?= $this->linkTitle; ?>" title="<?= $this->linkTitle; ?>" class="<?=$this->linkClass; ?>"<?= $this->target; ?><?= $this->linkAttributes; ?>>
			<?= $this->link; ?>
		</a>
	<?php endif; ?>
<?php endif; ?>