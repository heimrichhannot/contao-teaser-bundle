{
  "name": "heimrichhannot/contao-teaser-bundle",
  "description": "A contao teaser content element, that provides the ability to link the element with pages, articles, files, downloads or external urls.",
  "keywords": [
    "contao",
    "teaser",
    "content",
    "element"
  ],
  "type": "contao-bundle",
  "homepage": "https://github.com/heimrichhannot/contao-teaser-bundle",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Heimrich & Hannot",
      "email": "digitales@heimrich-hannot.de",
      "homepage": "http://www.heimrich-hannot.de",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/heimrichhannot/contao-teaser-bundle/issues"
  },
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13 || ^5.0",
    "doctrine/dbal": "^2.13 || ^3.0",
    "heimrichhannot/contao-utils-bundle": "^2.28 || ^3.0",
    "symfony/dependency-injection": "^5.4 || ^6.0 || ^7.0",
    "symfony/security-core": "^5.4 || ^6.0 || ^7.0"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\ContaoTeaserBundle\\": "src/"
    }
  },
  "replace": {
    "heimrichhannot/contao-teaser": "*"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\ContaoTeaserBundle\\ContaoManager\\Plugin"
  },
  "require-dev": {
    "rector/rector": "^2.0",
    "contao/contao-rector": "dev-main",
    "symplify/easy-coding-standard": "^12.5",
    "phpstan/phpstan": "^2.1",
    "phpstan/phpstan-symfony": "^2.0",
    "contao/manager-plugin": "^2.13"
  }
}
